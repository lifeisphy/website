<% if(show_time) { %>
  <div class="time">Created: <%- t.create_time  || 'undefined' %></div><div class="time">Updated: <%- t.alter_time || 'undefined' %></div>
  
<% } %>

<% if (tags && tags.length > 0) { %>
<div class="tags">
  Tags: 
  <% if (Array.isArray(tags)) { %>
    <% tags.forEach(function(tag, idx) { %>
      <a href="<%- tagsurl %>/<%- encodeURIComponent(tag) %>" data-partial="true"><%- tag %></a>
      <%= idx < tags.length - 1 ? ', ' : '' %>
    <% }) %>
  <% } else if (typeof tags === 'string') { %>
    <a href="<%- tagsurl %>/<%- encodeURIComponent(tags) %>" data-partial="true"><%- tags %></a>
  <% } %>
</div>
<% } %>

<% if(show_header) { %>
  <div class="header">
  </div>
<% } %>
<!-- add a separating line-->
<div class="line"></div>

<%- content %>
<% if(show_footer) { %>
  <div class="footer">
  </div>
<% } %>

